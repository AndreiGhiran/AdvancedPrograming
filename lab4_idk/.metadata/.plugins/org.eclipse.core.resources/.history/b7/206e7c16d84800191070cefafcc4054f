package lab4;

import java.io.DataInputStream;
import java.io.FileInputStream;
import java.util.ArrayList;

public class Catalog extends Graph{

	String path;
	ArrayList<Graph> graphs = new ArrayList<Graph>();
	
	Catalog(String path)
	{
		this.path=path;
	}
	
	public void add(Graph graph) {
		
		graphs.add(graph);
	}

	

}
